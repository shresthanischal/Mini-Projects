    "import pyshorteners"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def shortener(link):\n",
    "    short = pyshorteners.Shortener()\n",
    "    short_link = short.tinyurl.short(link)\n",
    "    return short_link"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter url:https://www.youtube.com/watch?v=WItLRHrY1-0&ab_channel=FormulaWorld\n",
      "Shortened link is https://tinyurl.com/y46opnt9\n"
     ]
    }
   ],
   "source": [
    "url = input(\"Enter url:\")\n",
    "final = shortener(url)\n",
    "print(f\"Shortened link is {final}\")"
   ]
  },
